{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_URL}"
  },
  "Jwt": {
    "Secret": "${JWT_SECRET}",
    "Issuer": "GroceryOrderingApp",
    "Audience": "GroceryOrderingAppUsers",
    "ExpirationMinutes": 480
  }
}
